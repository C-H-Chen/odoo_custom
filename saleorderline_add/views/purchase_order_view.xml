<odoo>
    <record id="view_purchase_order_form_inherit_vendor_part_no" model="ir.ui.view">
        <field name="name">purchase.order.form.inherit.vendor.part.no</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            
            <xpath expr="//field[@name='order_line']/tree/field[@name='name']" position="before">
                <field name="vendor_part_no" string="廠商料號"/>
            </xpath>

        </field>
    </record>
</odoo>
